{
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/health",
      "destination": "/api"
    },
    {
      "source": "/docs",
      "destination": "/api"
    },
    {
      "source": "/docs/(.*)",
      "destination": "/api"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}